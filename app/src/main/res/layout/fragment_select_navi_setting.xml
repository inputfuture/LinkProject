<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/size_48dp"
        android:background="@color/transparent_5">

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="@dimen/size_40dp"
            android:layout_height="@dimen/size_40dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/size_10dp"
            android:layout_marginRight="@dimen/size_10dp"
            android:scaleType="center"
            android:background="@drawable/button_circle_selector"
            android:src="@mipmap/back_icon" />

        <TextView
            android:id="@+id/tv_line"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/size_5dp"
            android:layout_marginTop="@dimen/size_5dp"
            android:layout_toRightOf="@id/iv_back"
            android:background="@color/transparent_30" />


        <TextView
            android:layout_toRightOf="@id/tv_line"
            android:layout_width="match_parent"
            android:text="@string/str_setting_choose_navi_app"
            android:textColor="@color/white"
            android:layout_marginRight="56dp"
            android:textSize="18sp"
            android:gravity="center"
            android:layout_height="match_parent" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="60dp"
        android:paddingRight="60dp">

        <RelativeLayout
            android:id="@+id/rl_ecolink"
            android:layout_width="match_parent"
            android:background="@drawable/music_list_selector"
            android:layout_height="@dimen/size_50dp">


            <ImageView
                android:id="@+id/iv_ecolink"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/set_naviset_sel" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dip"
                android:gravity="center_vertical"
                android:text="@string/app_name"
                android:textColor="@color/transparent_60"
                android:textSize="17sp" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_0.5dp"
            android:background="@color/list_line"></View>
        <RelativeLayout
            android:id="@+id/rl_gaode"
            android:layout_width="match_parent"
            android:background="@drawable/music_list_selector"
            android:layout_height="@dimen/size_50dp">


            <ImageView
                android:id="@+id/iv_gaode_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/set_naviset_sel" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dip"
                android:gravity="center_vertical"
                android:text="@string/main_nav_gaode_navi"
                android:textColor="@color/transparent_60"
                android:textSize="17sp" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_0.5dp"
            android:background="@color/list_line"></View>
        <RelativeLayout
            android:id="@+id/rl_baidu"
            android:layout_width="match_parent"
            android:background="@drawable/music_list_selector"
            android:layout_height="@dimen/size_50dp">


            <ImageView
                android:id="@+id/iv_baidu_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/set_naviset_sel" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dip"
                android:gravity="center_vertical"
                android:text="@string/main_nav_baidu_navi"
                android:textColor="@color/transparent_60"
                android:textSize="17sp" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_0.5dp"
            android:background="@color/list_line"></View>






    </LinearLayout>


</LinearLayout>
